@using Orchard.ContentManagement
@model Associativy.Frontends.Models.IGraphAspect
           
@foreach (var node in Model.As<Associativy.Frontends.Engines.Dracula.Models.DraculaPart>().Nodes)
{
    @Html.Raw("graph.addNode(\"" + node.Value.ContentItem.Id + "\", { label : \"" + HttpUtility.JavaScriptStringEncode(node.Value.Label) + "\" });")
}